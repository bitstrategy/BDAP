@using YYBDAP.Model
@model List<YYBDAP.Model.User>

@{ 
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div>
    <table class="table" border="1" cellpadding="0" cellspacing="0">
        <tr>
            <th>用户ID</th>
            <th>用户名</th>
            <th>描述</th>
            <th>邮箱</th>
            <th>注册时间</th>
            <th>上次登录时间</th>
            <th>上次登录IP</th>
        </tr>
        @foreach (var u in Model)
            {
            <tr>
                <td>@u.UserId</td>
                <td>@u.UserName</td>
                <td>@u.Description</td>
                <td>@u.EMail</td>
                <td>@u.RegistrationTime</td>
                <td>@u.LastLoginTime</td>
                <td>@u.LastLoginIP</td>
            </tr>
        }
    </table>
</div>
